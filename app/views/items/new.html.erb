<h1>Create new item <%= @item.id %></h1>

<%= form_for @item, html: {id: "new-item", multipart: true} do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :description %>
  <%= f.text_field :description %>
  <%= f.label :location %>
  <%= f.text_field :location %>
  <%= f.file_field :image %>
  <%= f.submit :submit %>
<% end %>

