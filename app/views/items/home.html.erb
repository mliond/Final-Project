<section id="services">
  <div class="container">
    <div class="row">
      <div class="col-lg-12 text-center">
        <%= image_tag "https://s3.amazonaws.com/testbucketfinalproject/statics/logo4.png", class: "logo-image" %>
        <h2 class="section-heading">Find awesome treasures near you</h2>
        <hr class="primary">
      </div>
    </div>
  </div>
  <div class="container">
    <div class="row">
      <div class="col-lg-4 text-center">
        <div class="service-box">
          <span class="threesteps glyphicon glyphicon-heart" aria-hidden="true"></span>
          <h3>Find items</h3>
          <p class="text-muted">Check your street, neighborhood, or entire city for posted items.</p>
        </div>
      </div>
      <div class="col-lg-4 text-center">
        <div class="service-box">
          <span class="threesteps glyphicon glyphicon-lamp" aria-hidden="true"></span>
          <h3>Get them!</h3>
          <p class="text-muted">Get a couple of friends and get the item from the specified location.</p>
        </div>
      </div>
      <div class="col-lg-4 text-center">
        <div class="service-box">
          <span class="threesteps glyphicon glyphicon-map-marker" aria-hidden="true"></span>
          <h3>Mark items as claimed</h3>
          <p class="text-muted">Other people will search for the item too. Let them know you have it already. </p>
        </div>
      </div>
    </div>
  </div>
</section>


<aside class="bg-dark">
    <div class="container text-center">
        <div class="call-to-action">
            <h2>Get free items in your area!</h2>
            <%= link_to "Get started!", items_path, class: "btn btn-default btn-xl wow tada" %>
        </div>
    </div>
</aside>




